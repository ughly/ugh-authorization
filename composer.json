        "permissions"
    ],
    "homepage": "https://github.com/ughly/ugh-authorization",
    "authors": [
        {
            "name": "Chris Yawman",
            "email": "christopher.yawman@neustar.biz"
        },
        {
            "name": "Joel Gilliland",
            "email": "joel.gilliland@neustar.biz"
